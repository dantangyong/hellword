<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mht.define.portal.cms.dao.SolrCoreMapDao">
	<resultMap id="coreMapResultMap" type="com.mht.define.portal.cms.entity.SolrCoreMap">  
	    <id column="id" property="id"/>  
	    <result column="corename" property="coreName"/>  
	    <result column="mapcode" property="mapCode"/>
	</resultMap>
	
	<resultMap id="fieldResultMap" type="com.mht.define.portal.cms.entity.SolrReturnField">  
	    <id column="id" property="id"/>  
	    <result column="corename" property="coreName"/>  
	    <result column="mapcode" property="mapCode"/>
	</resultMap>  
	  
	<select id="getCoreMap" resultMap="coreMapResultMap">
		SELECT * FROM solr_coremap	
	</select>
	<select id="getReturnFields" resultMap="fieldResultMap">
		SELECT * FROM solr_return_field	
	</select>
	
